{
  "name": "vscode-code-share",
  "displayName": "vscode-code-share",
  "description": "",
  "version": "0.0.1",
  "publisher": "vscode-samples",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-extension-samples"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.saveCode",
        "title": "Save Selected Code"
      },
      {
        "command": "codeMarketView.showItem",
        "title": "Show Item"
      },
      {
        "command": "savedCodeView.publishCode",
        "title": "Publish Code"
      },
      {
        "command": "extension.refreshMarket",
        "title": "Refresh Market"
      },
      {
        "command": "extension.addCodeToSaved",
        "title": "Add Code to Saved"
      },
      {
        "command": "savedCodeView.editCode",
        "title": "Edit Saved Code"
      },
      {
        "command": "cowsay.say",
        "title": "cowsay"
      },
      {
        "command": "cowsay.backwards",
        "title": "cowsay (â†¹)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "codeMarketContainer",
          "title": "Code Market",
          "icon": "resources/code-market-icon.svg"
        }
      ]
    },
    "views": {
      "codeMarketContainer": [
        {
          "id": "codeMarketView",
          "name": "Code Market"
        },
        {
          "id": "savedCodeView",
          "name": "Saved Codes"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "savedCodeView.publishCode",
          "when": "view == savedCodeView",
          "group": "inline"
        },
        {
          "command": "savedCodeView.editCode",
          "when": "view == savedCodeView",
          "group": "inline"
        }
      ],
      "view/title": [
        {
          "command": "extension.refreshMarket",
          "when": "view == codeMarketView",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "extension.addCodeToSaved",
          "when": "editorHasSelection",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\""
  },
  "devDependencies": {
    "@types/node": "^18",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^7.14.0",
    "@typescript-eslint/parser": "^7.14.0",
    "eslint": "^8.26.0",
    "ts-loader": "^7.0.5",
    "typescript": "^5.5.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
